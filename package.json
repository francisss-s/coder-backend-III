{
  "name": "coder-backend-ii",
  "version": "1.0.0",
  "description": "Repo  para entrega de coder backend II",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node ./index.js --env production",
    "dev": "nodemon ./index.js --env development",
    "mocha": "mocha ./test/mocha",
    "chai": "chai ./test/chai",
    "super": "supertest ./test/supertest",
    "docker:build": "docker build -t franzisz/coder3 .",
    "docker:run": "docker run -d -p 3035:3035 --name coder3-container --env-file .env.production franzisz/coder3",
    "docker:logs": "docker logs -f coder3-container",
    "docker:stop": "docker stop coder3-container && docker rm coder3-container",
    "docker:push": "docker push franzisz/coder3"
  },
